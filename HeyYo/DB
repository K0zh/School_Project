drop table message purge ;
create table message(
	ms_num number primary key,
	sender varchar2(20) not null,
	receiver varchar2(20) not null,
	ms_content varchar2(200) not null,
	ms_date date default sysdate,
	ms_state varchar2(2) default 'n'
);

drop sequence ms_seq;
create sequence ms_seq start with 1 increment by 1 nocache ;
